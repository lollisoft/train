        MENUITEM "Lin&ks...",                   ID_OLE_EDIT_LINKS
        MENUITEM "<<OLE VERBS GO HERE>>",       ID_OLE_VERB_FIRST
    END
    POPUP "&Zeichnen"
    BEGIN
        MENUITEM "&Selektieren",                ID_DRAW_SELECT
        MENUITEM "&Text",                       ID_DRAW_TEXT
    END
    POPUP "&Bahnhof"
    BEGIN
        MENUITEM "&Dazu",                       ID_DRAW_BAHNHOF
        MENUITEM "&Verbinden",                  ID_CONNECT_BAHNHOF
    END
    POPUP "&Fahrplan"
    BEGIN
        MENUITEM "&Zugfahrt eingeben",          ID_DRAW_MAKE_FAHRPLAN
        MENUITEM "Zugfahrt &loeschen",          ID_DRAW_DELETE_FAHRPLAN
        MENUITEM "Über&sicht der Zugfahrten",   ID_VIEW_ZUGFAHRTEN
    END
    POPUP "Fahr&karte"
    BEGIN
        MENUITEM "&Auswählen",                  ID_CHOOSE_FAHRKARTE
        MENUITEM "&Löschen",                    ID_DELETE_FAHRKARTE
    END
/*    POPUP "&Kunden"
    BEGIN
        MENUITEM "&Eingabe",                    ID_INPUT_KUNDE
        MENUITEM "&Löschen",                    ID_DELETE_KUNDE
    END
*/    POPUP "&Objekt"
    BEGIN
        MENUITEM "&Line Color...",              ID_OBJECT_LINECOLOR
        MENUITEM "&Fill Color...",              ID_OBJECT_FILLCOLOR
        MENUITEM SEPARATOR
        MENUITEM "Move To Front\tCtrl+Plus",    ID_OBJECT_MOVETOFRONT
        MENUITEM "Move To Back\tCtrl+Minus",    ID_OBJECT_MOVETOBACK
        MENUITEM "Move Forward\tPlus",          ID_OBJECT_MOVEFORWARD
        MENUITEM "Move Back\tMinus",            ID_OBJECT_MOVEBACK
    END
    POPUP "&Ansicht"
    BEGIN
        MENUITEM "&Grid Lines\tG",              ID_VIEW_GRID
        MENUITEM "&Paper Color...",             ID_VIEW_PAPERCOLOR
        MENUITEM "Show &Objects",               ID_VIEW_SHOWOBJECTS
        MENUITEM SEPARATOR
        MENUITEM "&Toolbar",                    ID_VIEW_TOOLBAR
        MENUITEM "&Status Bar",                 ID_VIEW_STATUS_BAR
    END
    POPUP "&Fenster"
    BEGIN
        MENUITEM "&Cascade",                    ID_WINDOW_CASCADE
        MENUITEM "&Tile",                       ID_WINDOW_TILE_HORZ
        MENUITEM "&Arrange Icons",              ID_WINDOW_ARRANGE
        MENUITEM "&Zoom",                       ID_ZOOM
        MENUITEM "100 &Prozent",                ID_HUNDERTPROZENT
        MENUITEM SEPARATOR
        MENUITEM "&Netz Edit Fenster",          ID_VIEW_NETZ_EDIT
        MENUITEM "Netz &Use Fenster",           ID_VIEW_NETZ_USE        
    END
    POPUP "&Hilfe"
    BEGIN
        MENUITEM "Über &Train...",           ID_APP_ABOUT
    END
