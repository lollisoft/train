IDR_DRAWCLTYPE MENU PRELOAD DISCARDABLE 
BEGIN
    POPUP "&Datei"
    BEGIN
        MENUITEM "&Neu\tCtrl+N",                ID_FILE_NEW
        //MENUITEM "Ö&ffnen...\tCtrl+O",          ID_FILE_OPEN
        MENUITEM "&Schließen",                  ID_FILE_CLOSE
        MENUITEM "&Sichern\tCtrl+S",            ID_FILE_SAVE
        MENUITEM "Sichern &Als...",             ID_FILE_SAVE_AS
        MENUITEM SEPARATOR
        MENUITEM "&Drucken...\tCtrl+P",         ID_FILE_PRINT
        MENUITEM "Druck&vorschau",              ID_FILE_PRINT_PREVIEW
        MENUITEM "Drucker &Einstellung...",     ID_FILE_PRINT_SETUP
        MENUITEM SEPARATOR
        MENUITEM "Recent File",                 ID_FILE_MRU_FILE1, GRAYED
        MENUITEM SEPARATOR
        MENUITEM "&Ende",                       ID_APP_EXIT
    END
    POPUP "&Verändern"
